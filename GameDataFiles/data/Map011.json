{"id":2,"name":"EV002","note":"","pages":[{"conditions":{"actorId":1,"actorValid":false,"itemId":1,"itemValid":false,"selfSwitchCh":"A","selfSwitchValid":false,"switch1Id":11,"switch1Valid":false,"switch2Id":1,"switch2Valid":false,"variableId":1,"variableValid":false,"variableValue":0},"directionFix":false,"image":{"characterIndex":0,"characterName":"","direction":2,"pattern":0,"tileId":0},"list":[{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["クローゼットだ。鍵がかかっていて開けられそうにない。"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["ぼく："]},{"code":401,"indent":0,"parameters":["「ん？下の方に何か書いてあるな」"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["『鍵はオリジナルが持っている』"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["ぼく："]},{"code":401,"indent":0,"parameters":["「意味は分からないけど…"]},{"code":401,"indent":0,"parameters":["　今まで調べたところをもう一度調べなおしてみよう」"]},{"code":121,"indent":0,"parameters":[10,10,0]},{"code":0,"indent":0,"parameters":[]}],"moveFrequency":3,"moveRoute":{"list":[{"code":0,"parameters":[]}],"repeat":true,"skippable":false,"wait":false},"moveSpeed":3,"moveType":0,"priorityType":1,"stepAnime":false,"through":false,"trigger":0,"walkAnime":true},{"conditions":{"actorId":1,"actorValid":false,"itemId":1,"itemValid":false,"selfSwitchCh":"A","selfSwitchValid":false,"switch1Id":11,"switch1Valid":true,"switch2Id":1,"switch2Valid":false,"variableId":1,"variableValid":false,"variableValue":0},"directionFix":false,"image":{"characterIndex":0,"characterName":"","direction":2,"pattern":0,"tileId":0},"list":[{"code":241,"indent":0,"parameters":[{"name":"Horror-ginen","volume":0,"pitch":100,"pan":0}]},{"code":250,"indent":0,"parameters":[{"name":"Key","volume":90,"pitch":100,"pan":0}]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["小さな鍵を使ってクローゼットを開けた。"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["ぼく："]},{"code":401,"indent":0,"parameters":["「何だこれ…」"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["血に塗れた手紙を入手した。"]},{"code":250,"indent":0,"parameters":[{"name":"Book2","volume":90,"pitch":100,"pan":0}]},{"code":242,"indent":0,"parameters":[1]},{"code":241,"indent":0,"parameters":[{"name":"Horror-ginen","volume":90,"pitch":100,"pan":0}]},{"code":101,"indent":0,"parameters":["",0,0,1]},{"code":401,"indent":0,"parameters":["もうダメだ。どうにもならない。"]},{"code":401,"indent":0,"parameters":["全てが分かったところで、俺には何もできやしない。"]},{"code":101,"indent":0,"parameters":["",0,0,1]},{"code":401,"indent":0,"parameters":["せっかく牢屋から逃げられたというのに"]},{"code":401,"indent":0,"parameters":["この足では、もう…。"]},{"code":401,"indent":0,"parameters":["クローゼットに隠れてはいるが、いずれ見つかるだろう。"]},{"code":101,"indent":0,"parameters":["",0,0,1]},{"code":401,"indent":0,"parameters":["これは遺書だ。"]},{"code":401,"indent":0,"parameters":["俺の名前は、多田野 武（ただの　たけし）。"]},{"code":401,"indent":0,"parameters":["次の”たけし”がこれを見つけて、無事に逃げ切ってくれる"]},{"code":401,"indent":0,"parameters":["ことを祈る。"]},{"code":101,"indent":0,"parameters":["",0,0,1]},{"code":401,"indent":0,"parameters":["まず、咲子の言う”たけし”とはあいつの元カレの事だ。"]},{"code":401,"indent":0,"parameters":["俺の事ではないし、これを読んでいる君の事でもない。"]},{"code":401,"indent":0,"parameters":["浮気をして、咲子をこっぴどく振った元カレを指す。"]},{"code":101,"indent":0,"parameters":["",0,0,1]},{"code":401,"indent":0,"parameters":["ややこしいから、ここでは”オリジナル”と呼ぶことにする。"]},{"code":101,"indent":0,"parameters":["",0,0,1]},{"code":401,"indent":0,"parameters":["咲子は浮気をしたオリジナルを許せなかった。"]},{"code":401,"indent":0,"parameters":["だから牢屋を作り、監禁し、拷問し、何度も謝罪をさせた。"]},{"code":101,"indent":0,"parameters":["",0,0,1]},{"code":401,"indent":0,"parameters":["しかし、咲子は拷問の加減を知らなかった。"]},{"code":401,"indent":0,"parameters":["咲子の気が済む前に、オリジナルはあっけなく死んで"]},{"code":401,"indent":0,"parameters":["しまった。"]},{"code":101,"indent":0,"parameters":["",0,0,1]},{"code":401,"indent":0,"parameters":["咲子はまだまだ謝罪が足りないと思った。"]},{"code":401,"indent":0,"parameters":["もっともっと謝罪してもらわないと、心の傷が癒えない。"]},{"code":401,"indent":0,"parameters":["そう思った咲子が次に取った行動は、同じ”たけし”と言う"]},{"code":401,"indent":0,"parameters":["名前の男を拉致しては拷問にかけるというものだった。"]},{"code":101,"indent":0,"parameters":["",0,0,1]},{"code":401,"indent":0,"parameters":["そう、俺たちはオリジナルの替わりなのだ。"]},{"code":401,"indent":0,"parameters":[""]},{"code":401,"indent":0,"parameters":["…拷問の最中、咲子がつぶやいた断片的な言葉を繋ぎ合わ"]},{"code":401,"indent":0,"parameters":["せて得られた情報はこれぐらいだ。"]},{"code":101,"indent":0,"parameters":["",0,0,1]},{"code":401,"indent":0,"parameters":["俺が何番目の”たけし”なのかは分からない。"]},{"code":401,"indent":0,"parameters":["ただ、オリジナルと違ってすぐ死なないところを見ると"]},{"code":401,"indent":0,"parameters":["ずいぶんとうまい加減で拷問できるようになったみたいだ。"]},{"code":401,"indent":0,"parameters":["…クソッタレ。"]},{"code":101,"indent":0,"parameters":["",0,0,1]},{"code":401,"indent":0,"parameters":["ああ、じゃがいもの臭いが漂ってきた。"]},{"code":401,"indent":0,"parameters":["料理を終えたあの女が、もうすぐ戻ってくる。"]},{"code":101,"indent":0,"parameters":["",0,0,1]},{"code":401,"indent":0,"parameters":["もうじゃがいもはうんざりだ。"]},{"code":401,"indent":0,"parameters":[""]},{"code":401,"indent":0,"parameters":["せめて最期に、美味しいビーフシチューが食べたかった…。"]},{"code":101,"indent":0,"parameters":["",0,0,1]},{"code":401,"indent":0,"parameters":["これを読んでいる君へ。"]},{"code":401,"indent":0,"parameters":["無事に脱出してくれ。"]},{"code":401,"indent":0,"parameters":["そしてこの家で起きている狂気の沙汰を、世間に…"]},{"code":401,"indent":0,"parameters":["俺たちの、遺体を、供養を…どうか…"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["文章はここで途切れている。"]},{"code":242,"indent":0,"parameters":[1]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["咲子："]},{"code":401,"indent":0,"parameters":["「たけしくん、何をしてるの？」"]},{"code":201,"indent":0,"parameters":[0,1,6,4,2,2]},{"code":0,"indent":0,"parameters":[]}],"moveFrequency":3,"moveRoute":{"list":[{"code":0,"parameters":[]}],"repeat":true,"skippable":false,"wait":false},"moveSpeed":3,"moveType":0,"priorityType":1,"stepAnime":false,"through":false,"trigger":0,"walkAn